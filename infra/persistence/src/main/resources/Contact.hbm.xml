<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
    <class name = "ecc.hibernate.xml.model.Contact" table="CONTACT">
      <id name = "id" column = "ID">
         <generator class = "identity">
          <param name="sequence">contact_id_seq</param>
       </generator>
      </id>
      <property name = "type" column = "TYPE" type = "string"/>
      <property name = "information" column = "INFORMATION" type = "string"/>
      <many-to-one name="person" class="ecc.hibernate.xml.model.Person" fetch="select">
        <column name="PERSON_ID" not-null="true" />
      </many-to-one>
    </class>
</hibernate-mapping>